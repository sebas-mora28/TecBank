<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".clientView.accounts.AccountClientFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <Button
            android:id="@+id/transactions_button"
            android:layout_width="218dp"
            android:layout_height="63dp"
            android:layout_marginStart="96dp"
            android:layout_marginBottom="48dp"
            android:onClick="admin_activity"
            android:text="Transacciones"
            app:layout_constraintBottom_toTopOf="@+id/transfers_button"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <Button
            android:id="@+id/transfers_button"
            android:layout_width="218dp"
            android:layout_height="63dp"
            android:layout_marginStart="96dp"
            android:layout_marginBottom="332dp"
            android:text="Transferencia"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>